{
  "expo": {
    "name": "Xpense",
    "slug": "Xpense",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cross.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/cross.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.piyush.Xpense"
    },
    "web": {
      "favicon": "./assets/cross1.png"
    }
  }
}
